$('#body').append("<div class='myPopup'></div>");
$('.myPopup').load("https://services.ingeniux.com/absciexv9redesigndss/prebuilt/custom/raq.html");

$(document).ready(function(){

    //  $("#nav").hide();
    //  $('#new-nav').insertAfter('nav#nav');


$("#nav").addClass("navbar navbar-default collapse navbar-collapse js-navbar-collapse");
$("#nav > div > ul").removeClass("left menu").addClass("nav navbar-nav");

/* products */
$("#nav > div > ul > li:nth-child(1)").removeClass(" mega").addClass("dropdown mega-dropdown dropdown-toggle");
$(".products-wrapper").closest(".mega-menu").removeClass("mega-menu");

/* technology */
$("#nav > div > ul > li:nth-child(2)").removeClass(" mega").addClass("dropdown mega-dropdown-technology dropdown-toggle");
$(".technology-wrapper").closest(".mega-menu").removeClass("mega-menu");
$("#nav > div > ul > li.dropdown.mega-dropdown-technology.dropdown-toggle > a").text("Technology").attr("title","Technology"); 

/* applications*/
$("#nav > div > ul > li:nth-child(3)").removeClass(" mega").addClass("dropdown mega-dropdown-applications dropdown-toggle");
$(".applications-wrapper").closest(".mega-menu").removeClass("mega-menu");

/* Diagnostics*/
$("#nav > div > ul > li:nth-child(4)").removeClass(" mega").addClass("dropdown mega-dropdown-diagnostics dropdown-toggle");
$(".diagnostics-wrapper").closest(".mega-menu").removeClass("mega-menu");
$("#nav > div > ul > li.dropdown.mega-dropdown-diagnostics.dropdown-toggle > a").text("Diagnostics").attr("title","Diagnostics");

/* Education*/
$("#nav > div > ul > li:nth-child(5)").removeClass(" mega").addClass("dropdown mega-dropdown-education dropdown-toggle");
$(".education-wrapper").closest(".mega-menu").removeClass("mega-menu");

/* support*/
$("#nav > div > ul > li:nth-child(6)").removeClass(" mega").addClass("dropdown mega-dropdown-support dropdown-toggle");
$("#nav > div > ul > li.dropdown.mega-dropdown-support.dropdown-toggle > a").text("Support").attr("title","Support");
$(".support-wrapper").closest(".mega-menu").removeClass("mega-menu");


/* Community*/
$("#nav > div > ul > li:nth-child(7)").removeClass(" mega").addClass("dropdown mega-dropdown-community dropdown-toggle");
$(".community-wrapper").closest(".mega-menu").removeClass("mega-menu");
$("#nav > div > ul > li.dropdown.mega-dropdown-community.dropdown-toggle > a").text("Community").attr("title","Community");


/* aboutus*/
$("#nav > div > ul > li:nth-child(8)").removeClass(" mega").addClass("dropdown mega-dropdown-aboutus dropdown-toggle");
$(".aboutus-wrapper").closest(".mega-menu").removeClass("mega-menu");

// request info button //
//$("#page > nav > div > ul > li.dropdown.mega-dropdown-aboutus").after('<span class="findMore"><a class="btn orange" href="https://sciex.com/x61109.xml">Request Info</a></span>')

    $(".mega-dropdown,.mega-dropdown-technology, .mega-dropdown-applications, .mega-dropdown-diagnostics, .mega-dropdown-education, .mega-dropdown-support, .mega-dropdown-community, .mega-dropdown-aboutus").hover(
        function() { $('.dropdown-menu', this).stop().fadeIn("fast");
        },
        function() { $('.dropdown-menu', this).stop().fadeOut("fast");
    });
});

</script>


