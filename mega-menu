  $(document).ready(function() {  
    var counter = 0; 
    /* Please edit links and text of the scroll bar here */
    var type = ["News","Press Releases","Events"];
    var details = ["Official Opening of SCIEX Centre of Distinction @ Protein & Proteomics Centre in Singapore","New X500B QTOF Mass Spectrometry System for Standardized Biotherapeutic Characterization","Register for the European Capillary Electrophoresis User Meeting in Berlin May 17-18 2017"];
    var cta = ["More Details >","Read More >","Register >"];
    var links=["x36168.xml","x53268.xml","x52765.xml"];
    /* End of text edit*/
    changeText();  
    go();
    window.addEventListener('resize', go); 

    setInterval(change, 3000); 

    function go(){    
     if(document.body.clientWidth>629){           
      $('.quotes').show();          
      $('.details').css( "width", function() {
        return Number($('#slider .slideshow').width()-$('.typeBg').width()- $('span.CTA').width()-68); 
      })   
    } 
    if(document.body.clientWidth<=629){
      $('.quotes').hide();
    } 
  }  
  function changeText() { 
    $('.type').html(type[1]);     
    $('.CTAText').html(cta[1]);    
    $('.detailsText').html(details[1]);  
    $('.details a').attr("href", links[1]); 
    $('.CTA a').attr("href", links[1]); 
  }   
  function change() {     
    $('.type').html(type[counter]);     
    $('.CTAText').html(cta[counter]);     
    $('.details a').attr("href", links[counter]); 
    $('.CTA a').attr("href", links[counter]); 
    $('.details').css( "width", function() {
     return Number($('#slider .slideshow').width()-$('.typeBg').width()- $('span.CTA').width()-68); 
   }) 
    $('.detailsText').html(details[counter]);
    counter++; 
    if(counter >= type.length) { counter = 0; }
  }
  $('.container-fluid>.row>div').addClass('col-lg-6 col-md-6 col-xs-12'); 
  $('.feature.clearfix>.content').addClass('col-lg-8 col-md-8 col-sm-8 col-xs-12');
  $('.feature.clearfix>.image').addClass('col-lg-4 col-md-4 col-sm-4 col-xs-12');
  $('.feature.clearfix>div').removeClass('col-md-6');
  //$('.row>div:first-child .feature>.image>img').attr("src", "https://images.sciex.com/education/CustExp_Wheel_150x150.jpg");
  //$('.row>div:last-child .feature>.image>img').attr("src", "https://images.sciex.com/education/Support-Icon_SciexU_150x150.jpg");
  $('.new-section-promo').contents().appendTo('.container-fluid');
  $('div.callouts').insertAfter('.product-section');
})  
</script>
<script src="https://services.ingeniux.com/absciexv9redesigndss/prebuilt/custom/raq.js"></script>
